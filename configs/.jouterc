{
    "port": 9000,
    "downstreams": {
        "legions": {
            "url": "https://legions.svc",
            "timeout": "10s"
        },
        "primarchs": {
            "url": "https://primarchs.svc:443",
            "timeout": "1m"
        }
    },
    "endpoints": {
        "/api/v1/primarch/jsonrpc": {
            "routing": "direct",
            "redirectTo": "primarches",
            "request": {
                "message": "asIs",
                "addHeaders": [
                    "X-Rpc-Method"
                ]
            },
            "response": {
                "message": "asIs"
            },
            "methods": [
                {
                    "name": "get_primarch",
                    "routing": "useMethod",
                    "request": {
                        "message": "shrink",
                        "addHeaders": [
                            "X-Rpc-Method",
                            "X-Rpc-Id"
                        ]
                    },
                    "response": {
                        "message": "expand"
                    }
                }
            ]
        }
    }
}